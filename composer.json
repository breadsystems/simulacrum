{
    "name": "tamayo/simulacrum",
    "description": "Open-source image manipulation service",
    "require": {
        "gumlet/php-image-resize": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "authors": [
        {
            "name": "Coby Tamayo",
            "email": "coby@tamayo.email"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "simulacrum:init": "bin/init.php"
    },
    "autoload": {
        "files": [
          "src/lib.php",
          "src/Api.php",
          "src/Image.php",
          "src/Ops.php",
          "src/Upload.php"
        ],
        "psr-4": {
            "Simulacrum\\": "src/"
        }
    },
    "autoload-dev": {
        "files": ["src/util.php"]
    }
}
